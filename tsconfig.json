{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "importHelpers": true,
      "jsx": "react",
      "esModuleInterop": true,
      "sourceMap": true,
      "baseUrl": ".",
      "strict": true,
      "paths": {
        "@/*": ["src/*"]
      },
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": false
    },
    "include": [
        "./globals.d.ts",
    ],
    "exclude": [
        "node_modules"
    ]
}
  